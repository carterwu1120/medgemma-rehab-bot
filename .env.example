YOUTUBE_CHANNEL_WHITELIST=
VIDEO_USE_YOUTUBE_API=1

# Backend API
BACKEND_HOST=0.0.0.0
BACKEND_PORT=9000
CORS_ORIGINS=http://localhost:3000
